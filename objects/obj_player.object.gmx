            <string>///show help controls
//works only in tutorial
if(obj_player_stats.level == 0) {
    //check to see if the player is touching the show x button tile
    x_tile = instance_place(x, y, obj_show_x);
    if(x_tile != noone) {
        if(obj_btn_x.sprite_index != sp_btn_x) {
            obj_btn_x.sprite_index = sp_btn_x;
            obj_btn_x.image_speed = 0.033;
        }
    } else {
        if(obj_btn_x.sprite_index != sp_blank) {
            obj_btn_x.sprite_index = sp_blank;
