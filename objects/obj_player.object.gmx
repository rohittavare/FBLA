        if(obj_btn_z.sprite_index != sp_btn_z) {
            obj_btn_z.sprite_index = sp_btn_z;
            obj_btn_z.image_speed = 0.033;
        }
    } else {
        if(obj_btn_z.sprite_index != sp_blank) {
            obj_btn_z.sprite_index = sp_blank;
        }
    }
    
    //check to see if the player is touching the show keys tile
    key_tile = instance_place(x, y, obj_show_keys);
    if(key_tile != noone) {
        if(obj_keys.sprite_index != sp_keys) {
            obj_keys.sprite_index = sp_keys;
            obj_keys.image_speed = 0.033;
        }
    } else {
        if(obj_keys.sprite_index != sp_blank) {
            obj_keys.sprite_index = sp_blank;
        }
    }
}
